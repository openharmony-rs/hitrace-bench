    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug executable 'hitrace-bench'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "run",
                    "--bin=hitrace-bench"
                ]
            },
            "args": []
        },
        {
            "name": "Debug runs.json",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "run",
                    "--bin=hitrace-bench"
                ]
            },
            "args": ["-r", "runs2.json"]
        },
        {
            "name": "Debug tmp file",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": ["run", "--bin=hitrace-bench"]
            },
            "args": ["--trace-file", "/tmp/app.ftrace"]
        },
        {
            "name": "Debug unit tests in executable 'hitrace-bench'",
            "type": "lldb",
            "request": "launch",
            "cargo": {
                "args": [
                    "test",
                    "--bin=hitrace-bench"
                ]
            }
        }
    ]
}